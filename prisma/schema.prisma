generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id            String   @id @default(cuid())
  name          String
  age           Int
  profilePic    String
  dob           DateTime
  bioVideo      String
  profilePicKey String
  bioVideoKey   String
}
